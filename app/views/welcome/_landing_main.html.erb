<!-- Header (Front Page) -->
<header id="intro-splash">
  <div class="container">
    <div class="row" data-hook>
      <%= breadcrumbs(@taxon) %>
      
        <div class="col-lg-12" data-hook>
          <div class="intro-text">
            <span class="name">Coming Soon</span>
            <hr class="star-light">
            <span class="skills">Shop Boldly For Your Style</span>
          </div>
        </div>
    </div>
  </div>
</header>

<%= render partial: 'welcome/about' %>
